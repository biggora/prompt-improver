{
  "name": "prompt-improver",
  "version": "1.0.0",
  "description": "An AI-powered application to analyze, optimize, and improve your prompts using multiple AI providers like Anthropic, OpenAI, Gemini, and Zhipu AI.",
  "license": "MIT",
  "keywords": [
    "ai",
    "prompt-engineering",
    "prompt-optimization",
    "nextjs",
    "anthropic",
    "openai",
    "google-gemini",
    "zhipu-ai"
  ],
  "author": "",
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest --no-watch",
    "test:watch": "vitest",
    "kill:app": "kill-app.cmd -p 3000",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,mjs,json,md,yml}\""
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.36",
    "@ai-sdk/google": "^3.0.21",
    "@ai-sdk/openai": "^3.0.25",
    "ai": "^6.0.69",
    "lucide-react": "^0.263.1",
    "next": "^16.0.0",
    "next-intl": "^4.8.2",
    "next-themes": "^0.4.6",
    "prettier": "^3.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zhipu-ai-provider": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.0.0",
    "fake-indexeddb": "^6.2.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.7.0",
    "vitest": "^3.2.4"
  }
}
